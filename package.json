{
  "name": "hnpwa-react",
  "private": true,
  "version": "1.0.1",
  "description": "HNPWA with React",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.config.js --env.dev --inline --watch",
    "start:firebase": "firebase serve --only hosting,functions",
    "analyze": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js --env.prod --profile --json > stats.json && yarn analyze:cache",
    "analyze:cache": "webpack-bundle-analyzer stats.json bundleDir=build",
    "build": "yarn build:client && yarn build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js --progress --env.prod",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js --progress",
    "deploy": "firebase deploy --project hnpwa-react --token=$FIREBASE_DEPLOY_TOKEN",
    "deploy:stage": "firebase deploy --project hnpwa-react-stage --token=$FIREBASE_DEPLOY_TOKEN",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint app test --ext .js,.jsx",
    "lint:scss": "bundle exec scss-lint app/",
    "ncu": "ncu",
    "precommit": "lint-staged",
    "prepush": "yarn lint",
    "test": "cross-env NODE_ENV=test jest --config jest.config.json",
    "test:coverage": "yarn test -- --coverage",
    "test:watch": "yarn test -- --watch",
    "coverage": "open ./coverage/lcov-report/index.html",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "gh-pages": "yarn gh-pages:clean && yarn gh-pages:build && yarn gh-pages:publish",
    "gh-pages:clean": "rimraf gh-pages",
    "gh-pages:build": "yarn build-storybook -o gh-pages",
    "gh-pages:publish": "git-directory-deploy --directory gh-pages --message '[skip ci]'"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint",
    "*.{css,scss}": "bundle exec scss-lint"
  },
  "dependencies": {
    "autotrack": "^2.4.1",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-router-server": "^4.2.2",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "transit-immutable-js": "^0.7.0",
    "transit-js": "^0.8.846",
    "uuid": "^3.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.13",
    "@storybook/addon-info": "^3.2.13",
    "@storybook/addon-knobs": "^3.2.13",
    "@storybook/addon-links": "^3.2.13",
    "@storybook/addon-options": "^3.2.13",
    "@storybook/react": "^3.2.13",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "csso-webpack-plugin": "^1.0.0-beta.9",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "firebase-tools": "^3.13.1",
    "git-directory-deploy": "^1.5.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "ignore-loader": "^0.1.2",
    "jest": "^21.2.1",
    "lint-staged": "^4.3.0",
    "nock": "^9.0.24",
    "node-fetch": "^1.7.3",
    "node-sass": "^4.5.3",
    "npm-check-updates": "^2.13.0",
    "npm-run-all": "^4.1.1",
    "postcss-loader": "^2.0.8",
    "preload-webpack-plugin": "^2.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0",
    "resolve-url-loader": "^2.1.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.3",
    "webpack-merge": "^4.1.0",
    "workbox-google-analytics": "^2.1.0",
    "workbox-webpack-plugin": "^2.1.0"
  }
}
